<?xml version="1.0" encoding="utf-8"?>
<manifest package="works.agus.songzter"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".NowPlaying"
            android:label="@string/nowPlaying">
        </activity>
        <activity
            android:name=".YourMusic"
            android:label="@string/your_music">
        </activity>
        <activity
            android:name=".Discover"
            android:label="@string/discover">
        </activity>
        <activity
            android:name=".SearchResults"
            android:label="@string/searchResults">
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
    </application>

</manifest>